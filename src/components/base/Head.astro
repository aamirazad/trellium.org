---
const { title } = Astro.props;

import { Font } from "astro:assets";
---

<link rel="sitemap" href="/sitemap-index.xml" />
<title>{title}</title>
<meta name="description" content="Building services to better the world." />

<!-- Web analytics -->
<script
	is:inline
	src="https://www.aamira.me/api/script.js"
	data-site-id="8"
	async
	defer></script>

<!-- Open Graph tags -->
<meta property="og:title" content="Trellium" />
<meta property="og:type" content="website" />
<meta property="og:url" content="https://trellium.org" />
<meta
	property="og:description"
	content="A community organization creating helpful services."
/>
